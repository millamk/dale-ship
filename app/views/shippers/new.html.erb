<h1>new shipper</h1>

<div class="container">
  <%= simple_form_for(@shipper) do |f| %>
    <div class="form-inputs offset-4 col-4">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :cnpj %>
    <%= f.input :job_title %>
    <%= f.input :company_name %>
    <%= f.submit 'Send', class: 'btn btn-primary btn-block' %>
  <% end %>
  </div>
</div>
