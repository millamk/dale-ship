<div class="container">
  <%= simple_form_for(@carrier, url: carriers_profile_path) do |f| %>
    <div class="form-inputs mt-3 offset-3 col-6">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :CNPJ %>
    <%= f.input :company_name %>
    <%= f.input :job_title %>
    <%= f.input :carrier_type, collection: ['SMB Forwarder', 'Global Forwarder'] %>
    <%= f.submit 'Submit', class: 'btn btn-primary btn-block' %>
  <% end %>
  </div>
</div>



